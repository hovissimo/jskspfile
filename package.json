{
  "name": "jskspfile",
  "version": "1.0.0",
  "description": "A node library for handling Kerbal Space Program data files",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "test": "istanbul.cmd cover node_modules/mocha/bin/_mocha -- --require babel-core/register"
  },
  "keywords": [
    "Kerbal"
  ],
  "author": "http://github.com/hovissimo",
  "license": "UNLICENSED",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "rules": {
       "no-var": 2,
       "prefer-const": 2
    },
    "settings": {
      "esversion": 6
    }
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
